{
  "Bypass that Paywall": {
    "regex1": "^(https?):\/\/(\\S*)$",
    "regex": "^(?!.*(12ft.))(.*)",
    "replacement": "https:\/\/12ft.io\/$2",
    "enabled": "true"
  },
  "Summarize This": {
    "regex1": "^(https?):\/\/(\\S*)$",
    "regex": "^(?!.*(smmry.))(.*)",
    "replacement": "https:\/\/smmry.com\/$2#&SM_LENGTH=10",
    "enabled": "true"
  },
  "Search Term": {
    "regex": "^(?!.*:)",
    "replacement": "https:\/\/search.brave.com\/search?q=$0"
  },
  "X to Twitter": {
        "regex": "^https?:\/\/(?:[a-z0-9-]+\\.)*?x.com\/(.*)",
        "replacement": "https:\/\/twitter.com\/$1",
        "enabled": "true"
  }
}
